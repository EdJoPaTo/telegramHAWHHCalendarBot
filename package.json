{
  "name": "hawhhcalendarbot-telegrambot",
  "version": "6.1.3",
  "private": true,
  "description": "A bot to configure your personal calendar of HAW Hamburg lectures",
  "author": {
    "name": "EdJoPaTo",
    "email": "calendarbot@hawhh.de",
    "url": "https://calendarbot.hawhh.de"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "array-filter-unique": "^2.1.0",
    "got": "^11.5.2",
    "json-stable-stringify": "^1.0.1",
    "telegraf": "^3.38.0",
    "telegraf-inline-menu": "^4.0.0",
    "telegraf-middleware-console-time": "^1.0.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.7.0",
    "@types/json-stable-stringify": "^1.0.32",
    "ava": "^3.5.0",
    "nyc": "^15.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "xo": "^0.33.0"
  },
  "scripts": {
    "start": "ts-node source/index.ts",
    "test": "tsc --sourceMap && xo && nyc ava"
  },
  "xo": {
    "semicolon": false,
    "rules": {
      "ava/no-ignored-test-files": "off",
      "prefer-named-capture-group": "off",
      "unicorn/no-fn-reference-in-iterator": "warn"
    },
    "overrides": [
      {
        "files": [
          "source/**/*.js"
        ],
        "rules": {
          "indent": [
            "error",
            2
          ]
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HAWHHCalendarBot/telegrambot.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HAWHHCalendarBot/telegrambot/issues"
  },
  "homepage": "https://calendarbot.hawhh.de"
}
