{
  "name": "telegramHAWHHCalendarBot",
  "version": "2.0.0",
  "private": true,
  "description": "A bot to configure your personal calendar of HAW Hamburg lectures",
  "author": "edjopato@gmail.com",
  "main": "index.js",
  "engines": {
    "node": ">=7.6"
  },
  "dependencies": {
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "telegraf": "^3.20.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "xo": "^0.22.0"
  },
  "scripts": {
    "ava": "ava",
    "start": "node index.js",
    "pretest": "xo",
    "test": "ava",
    "xo": "xo"
  },
  "config": {
    "tokenpath": "/run/secrets/hawhhcalendarbot-token.txt",
    "tokenpathdebug": "token.txt"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "require-await": "error"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HAWHHCalendarBot/telegrambot.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HAWHHCalendarBot/telegrambot/issues"
  },
  "homepage": "https://calendarbot.hawhh.de"
}
