{
  "name": "telegramHAWHHCalendarBot",
  "version": "2.0.0",
  "private": true,
  "description": "A bot to configure your personal calendar of HAW Hamburg lectures",
  "author": "edjopato@gmail.com",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "json-stable-stringify": "^1.0.1",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "telegraf": "^3.23.0",
    "telegraf-inline-menu": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "xo": "^0.23.0"
  },
  "scripts": {
    "ava": "ava",
    "ava:watch": "ava -w",
    "xo": "xo",
    "start": "node index.js",
    "pretest": "xo",
    "test": "ava"
  },
  "config": {
    "tokenpath": "/run/secrets/hawhhcalendarbot-token.txt",
    "tokenpathdebug": "token.txt"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "overrides": [
      {
        "files": [
          "parts/changes-inline.js"
        ],
        "rules": {
          "camelcase": [
            "warn",
            {
              "properties": "never"
            }
          ]
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HAWHHCalendarBot/telegrambot.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HAWHHCalendarBot/telegrambot/issues"
  },
  "homepage": "https://calendarbot.hawhh.de"
}
