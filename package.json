{
  "name": "hawhhcalendarbot-telegrambot",
  "version": "3.7.1",
  "private": true,
  "description": "A bot to configure your personal calendar of HAW Hamburg lectures",
  "author": "edjopato@gmail.com",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "json-stable-stringify": "^1.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "telegraf": "^3.29.0",
    "telegraf-inline-menu": "^3.5.2"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "nyc": "^14.0.0",
    "xo": "^0.24.0"
  },
  "scripts": {
    "ava": "ava",
    "ava:watch": "ava -w",
    "nyc": "nyc",
    "xo": "xo",
    "start": "node index.js",
    "pretest": "xo",
    "test": "nyc ava"
  },
  "config": {
    "tokenpath": "/run/secrets/hawhhcalendarbot-token.txt",
    "tokenpathdebug": "token.txt"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "overrides": [
      {
        "files": [
          "parts/changes-inline.js"
        ],
        "rules": {
          "camelcase": "off"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HAWHHCalendarBot/telegrambot.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HAWHHCalendarBot/telegrambot/issues"
  },
  "homepage": "https://calendarbot.hawhh.de"
}
